<?xml version="1.0"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns:mx="library://ns.adobe.com/flex/mx"
    skinClass="skins.StripedSkin"
    title="View Skinned IR"
    >

    <fx:Script><![CDATA[
        import vo.ListData;
        ]]></fx:Script>

    <s:navigationContent>
        <s:Button click="navigator.popView()" label="Back"/>
    </s:navigationContent>

    <s:actionContent>
        <s:Button click="navigator.pushView(Transparency_View_Skinned_IconItemRenderer)" label="Next"/>
    </s:actionContent>

    <s:List width="100%"
            height="100%"
            dataProvider="{ListData.arrayList}"
            contentBackgroundAlpha="0"
            horizontalScrollPolicy="off">
        <s:itemRenderer>
            <fx:Component>
                <s:ItemRenderer autoDrawBackground="false">
                    <s:BitmapImage id="image" width="500" height="500" source="{data.image}" alpha=".5" smooth="true"/>
                </s:ItemRenderer>
            </fx:Component>
        </s:itemRenderer>
    </s:List>

</s:View>
